function onSubmit(offset) {
  squery.setOffset(offset);
  var result = true;
  console.log("Submit pressed.");
  var qval = $("#searchinput").val();
  console.log("Query is "+qval);
  squery.setQuery(qval);
  squery.setSessionId("20A0B664397101AE771858052061D467");
  var form = $("#searchform");
  if (form.length > 0) {
    console.log("Appending serialized form data.");
    form.append("<input type='hidden' name='jsondata' value='"+squery.serialize()+"'>");
    console.log("Submit!");
    form.submit();
    result = true;
  } else {
    console.log("Form not found!")
    result = false;
  }
  
  return result;
}

