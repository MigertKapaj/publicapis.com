	//find and replace link in text
	function replaceLink(text) {
		var pattern = /(HTTP:\/\/|HTTPS:\/\/)([a-zA-Z0-9.\/&?_=!*,;\(\)+-]+)/ig;
		var replace = "";
		var result = (text != null) ? text.replace(pattern, replace) : "";
		if (result.length > 700) {
			result = result.substr(0,700)+'...';
			}
		return result;
	}

	//set pagination
	function setPaginagion(length, page) {
		var tmp = length % 10;
		length = Math.floor(length / 10);
		if (tmp > 0)
			length++;

		$('.pagination').bootpag({
			total : length, // total pages
			page : (page + 1), // default page
			maxVisible : 10, // visible pagination
			leaps : false
		// next/prev leaps through maxVisible
